int main()
{
#ifdef llt
	freopen("15.in","r",stdin);
	freopen("15.out","w",stdout);
#endif
	while(1)
	{
		int t;
		scanf("%d",&t);
		if(t == -1) break;
		int y = gety(t);
		int m = getm(t);
		int d = getd(t);


}

