#include<stdio.h>
#include<algorithm>
#include<string.h>
#include<math.h>
using namespace std;

int gety(int &t)
{
	int y400 = t / (400 * 365 + 100 - 3);
	t %= (400 * 365 + 97);
	int y100 = t / (100 * 365 + 24);
	t %= 100 * 365 + 24;
	int y4 = t / (4 * 365 + 1);
	t %= 4 * 365 + 1;
	int y = 0;
	if(y > 366) y = 1 ,t -= 366,y += t / 365,t %= 365;
	else y = 0;
	return 400 * y400 + 100 * y100 + y4 * 4 + y;
}

